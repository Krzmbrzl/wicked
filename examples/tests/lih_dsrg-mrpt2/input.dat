import forte

molecule {
Li
H 1 1.6
symmetry c1
}

set {
  restricted_docc [1]
  active [2]
  mp2_type conv
  basis 6-31G
  scf_type pk
  e_convergence 13
  mcscf_e_convergence 11
  mcscf_maxiter 100
  mcscf_r_convergence 1.0e-6
  mcscf_diis_start 20          
}

ecasscf, cas = energy('casscf',return_wfn=True)

set forte {
  active_space_solver fci
  correlation_solver dsrg-mrpt2
  restricted_docc [1]
  active [2]
}

energy('forte',ref_wfn=cas)
